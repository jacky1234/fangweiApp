<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleView">
        <attr name="titleView_left_drawable" format="reference" />
        <attr name="titleView_left_text" format="reference" />
        <attr name="titleView_title_text" format="reference" />
        <attr name="titleView_left_text_color" format="color" />
        <attr name="titleView_middle_text_color" format="color" />
        <attr name="titleView_right_text" format="reference" />
        <attr name="titleView_right_text_color" format="color" />
        <attr name="titleView_visible_type" format="integer">
            <enum name="left_middle" value="1" />
            <enum name="all" value="2" />
            <enum name="left" value="3" />
        </attr>
    </declare-styleable>
</resources>